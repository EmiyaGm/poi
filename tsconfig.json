{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ESNext", "DOM"],
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "views/*": ["./views/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
  }
}
